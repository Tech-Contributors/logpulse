{
    "name": "techcontributors/logpulse",
    "description": "Centralized audit logging SaaS for Laravel applications.",
    "type": "library",
    "license": "MIT",

    "authors": [
        {
            "name": "Tech Contributors",
            "email": "support@techcontributors.com"
        }
    ],

    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/queue": "^9.0|^10.0|^11.0|^12.0"
    },

    "autoload": {
        "psr-4": {
            "TechContributors\\LogPulse\\": "src/"
        },
        "files": [
            "src/LogPulseFacadeStub.php"
        ]
    },

    "extra": {
        "laravel": {
            "providers": [
                "TechContributors\\LogPulse\\LogPulseServiceProvider"
            ],
            "aliases": {
                "LogPulse": "TechContributors\\LogPulse\\Facades\\LogPulse"
            }
        }
    },

    "minimum-stability": "stable",
    "prefer-stable": true
}